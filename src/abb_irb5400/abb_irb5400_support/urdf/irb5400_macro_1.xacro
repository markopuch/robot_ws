        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit effort="100" lower="-2.617" upper="2.617" velocity="2.3911"/>
    </joint>

    
    <joint name="${prefix}joint2" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
